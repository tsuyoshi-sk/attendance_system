[flake8]
max-line-length = 120
extend-ignore = E203, W503
exclude = 
    .git,
    __pycache__,
    build,
    dist,
    venv,
    .venv,
    migrations,
    .tox,
    .eggs,
    *.egg-info,
    .pytest_cache,
    .mypy_cache,
    htmlcov,
    docs,
    scripts,
    tests,
per-file-ignores =
    __init__.py:F401
    backend/app/models/*.py:F401
    backend/app/api/*.py:F401